<h1> <%= current_user.name %> is currently following: </h1>
<% @github_followings.return_followings.each do |following| %>
<div class="following">
  <h2 class="username"><%= following.username %></h2><br>
  <img src="<%= following.image %>" alt="prof_pic"><br>
  <h4><%= following.username %>'s recent commits</h4>
</div>
  <% @github_followings.followings_activities(following.username).each do |commit| %>
    Repo Affected: <%= commit.repo %><br>
    Link to Commit: <a href="<%= commit.url %>"><%= commit.url %></a><br>
    Time: <%= commit.time.strftime("%Y.%m.%d at %I:%M%p") %><br>
    Commit Message**: <%= commit.commit_message %><br><br>
  <% end %>
<% end %>
